{
  "name": "transifex-loader",
  "version": "3.3.0",
  "description": "Webpack loader to fetch imported translation files from transifex.",
  "main": "dist/transifex-loader.js",
  "scripts": {
    "test": "npm run lint && nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint src/ test/",
    "build": "webpack",
    "prepublish": "npm run build"
  },
  "keywords": [
    "transifex",
    "webpack",
    "loader"
  ],
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "loader-utils": "^1.1.0",
    "mz": "^2.4.0",
    "transifex-api-es6": "^0.2.3",
    "transifex-config": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-rewrite": "0.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2017": "^6.22.0",
    "codecov": "^2.0.1",
    "eslint": "^3.6.1",
    "eslint-plugin-freaktechnik": "^2.0.0",
    "nyc": "^10.0.0",
    "random-string": "^0.2.0",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.3.3"
  },
  "bugs": {
    "url": "https://github.com/freaktechnik/transifex-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/freaktechnik/transifex-loader.git"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-core/register"
    ]
  },
  "babel": {
    "presets": [
      "es2017",
      "@ava/stage-4"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-runtime",
      [
        "module-rewrite",
        {
          "replaceFunc": "./test/_rewrite-modules.js"
        }
      ]
    ]
  }
}
