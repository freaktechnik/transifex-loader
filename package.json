{
  "name": "transifex-loader",
  "version": "2.6.1",
  "description": "Webpack loader to fetch imported translation files from transifex.",
  "main": "dist/transifex-loader.js",
  "scripts": {
    "test": "npm run lint && nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint src/ test/",
    "build": "webpack",
    "prepublish": "npm run build"
  },
  "keywords": [
    "transifex",
    "webpack",
    "loader"
  ],
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-freaktechnik": "^1.2.1",
    "loader-utils": "^0.2.15",
    "promisify-call": "^1.0.0",
    "regenerator-runtime": "^0.9.5",
    "transifex-api-es6": "^0.2.2"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-rewrite": "0.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "codecov": "^1.0.1",
    "eslint": "^3.2.2",
    "eslint-plugin-freaktechnik": "^1.0.1",
    "nyc": "^8.0.0",
    "random-string": "^0.1.2",
    "webpack": "^1.13.1"
  },
  "bugs": {
    "url": "https://github.com/freaktechnik/transifex-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/freaktechnik/transifex-loader.git"
  },
  "ava": {
    "babel": "inherit",
    "require": "babel-register"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-runtime",
      [
        "module-rewrite",
        {
          "replaceFunc": "./test/_rewrite-modules.js"
        }
      ]
    ]
  }
}
