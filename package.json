{
  "name": "transifex-loader",
  "version": "6.0.0",
  "description": "Webpack loader to fetch imported translation files from transifex.",
  "exports": {
    "import": "./src/index.mjs"
  },
  "type": "module",
  "scripts": {
    "test": "npm run lint && nyc ava",
    "lint:js": "eslint --ext js,mjs src/ test/",
    "lint:ec": "eclint check $(git ls-files)",
    "lint": "npm run lint:js && npm run lint:ec"
  },
  "keywords": [
    "transifex",
    "webpack",
    "loader"
  ],
  "author": "Martin Giger (https://humanoids.be)",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.2.10",
    "transifex-config": "^5.0.0"
  },
  "devDependencies": {
    "@freaktechnik/eslint-config-node": "^9.0.3",
    "@freaktechnik/eslint-config-test": "^9.0.1",
    "ava": "^4.3.3",
    "eclint": "^2.8.1",
    "eslint": "^8.23.0",
    "nyc": "^15.1.0",
    "random-string": "^0.2.0",
    "sinon": "^14.0.0"
  },
  "bugs": {
    "url": "https://github.com/freaktechnik/transifex-loader/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/freaktechnik/transifex-loader.git"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "engines": {
    "node": ">= 12.22.0"
  },
  "sideEffects": false
}
